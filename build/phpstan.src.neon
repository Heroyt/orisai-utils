includes:
	- phpstan.base.neon

parameters:
	level: max
	resultCachePath: %currentWorkingDirectory%/var/build-tools/PHPStan/resultCache.src.php

	ignoreErrors:
		- message: '#^Parameter \#1 \$json of function json_decode expects string, string\|false given\.$#'
		  path: %currentWorkingDirectory%/src/Dependencies/PackageRequired.php
